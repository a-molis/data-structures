{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "build",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"${file}",
				"-o",
				"build/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${workspaceRoot}/questions/"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"type": "shell",
			"label": "run",
			"command": "build/${fileBasenameNoExtension}",
			"options": {
				"cwd": "${workspaceRoot}/questions/"
			},
			"dependsOn": ["build"]
		}
	]
}